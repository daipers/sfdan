{
  "name": "sfdan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "STATIC_EXPORT=true npm run build",
    "start": "next start",
    "lint": "eslint \"src/instrumentation.ts\" \"src/lib/config.ts\" \"src/lib/health.ts\" \"src/app/api/health/route.ts\" \"sentry.*.config.ts\" \"next.config.ts\" --no-error-on-unmatched-pattern",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "predeploy": "echo '' > out/.nojekyll && echo 'Static site generated for GitHub Pages'",
    "deploy:gh-pages": "npm run build:static && npm run predeploy && gh-pages -d out -b gh-pages -f",
    "deploy:local": "npm run build:static && npm run predeploy"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.39.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.97.0",
    "@tanstack/react-table": "^8.21.3",
    "@tremor/react": "^4.0.0-beta-tremor-v4.4",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "autoprefixer": "^10.4.24",
    "date-fns": "^4.1.0",
    "next": "^16.1.6",
    "nuqs": "^2.8.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "get-shit-done-cc": "^1.20.5",
    "gh-pages": "^6.1.1",
    "jsdom": "^28.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
